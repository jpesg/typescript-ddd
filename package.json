{
  "name": "tdd-ts",
  "version": "1.0.0",
  "description": "project example",
  "main": "index.js",
  "scripts": {
    "dev:user:backend": "cross-env NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/apps/user/backend/start.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=dev tsnd --ignore-watch node_modules ./index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "test": "npm run test:features",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:features": "npm run test:user:backend:features",
    "test:user:backend:features": "cross-env NODE_ENV=test cucumber-js -p user_backend",
    "build": "npm run build:clean && npm run build:tsc && npm run build:di",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' dist/src",
    "build:clean": "rm -r dist; exit 0",
    "command:users:rabbitmq": "NODE_ENV=production ts-node src/apps/user/backend/command/run.configure.rabbitmq.command",
    "dev:backoffice:frontend": "cd ./src/apps/backoffice/frontend && npm start",
    "dev:backoffice:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/apps/backoffice/backend/start.ts",
    "build:backoffice:frontend": "cd ./src/apps/backoffice/frontend && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "8.11.1",
    "@faker-js/faker": "7.6.0",
    "@types/amqplib": "0.10.1",
    "@types/compression": "1.7.2",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.17",
    "@types/glob": "8.0.1",
    "@types/helmet": "4.0.0",
    "@types/jest": "^29.4.0",
    "@types/mongodb": "4.0.7",
    "@types/node": "18.13.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "amqplib": "0.10.3",
    "awilix": "8.0.0",
    "bson": "5.0.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "glob": "8.1.0",
    "helmet": "6.0.1",
    "http-status": "1.6.2",
    "mongodb": "5.1.0",
    "node-dependency-injection": "2.6.11",
    "typeorm": "^0.3.14",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "zod": "3.20.6"
  }
}
