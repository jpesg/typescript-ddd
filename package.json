{
  "name": "tdd-ts",
  "version": "1.0.0",
  "description": "project example",
  "main": "index.js",
  "scripts": {
    "dev:user:backend": "cross-env NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/app/user/backend/start.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "dev": "cross-env NODE_ENV=dev tsnd --ignore-watch node_modules ./index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\""
  },
